    $(document).ready(function () {
        loadnotcancelledloi();
        $('#propertynameselection').hide();
        $('#aptnoselection').hide();
        $('#divOther').hide();
        $('#btnadd').hide();
        $('#table2').hide();
        $("#cash").prop("disabled", true);
        $("#knet").prop("disabled", true);
        $("#cheque").prop("disabled", true);
        $("#bt").prop("disabled", true);

        $("#loino1").change(function () {
            var loino1 = $('#loino1 :selected').val();
            $.ajax({
                type: "POST",
                url: "@Url.Action("LoadDetFromLoiInfo", "Accounts")",
                data: { loino1: loino1 },
                success: function (response) {
                    //alert(response)
                    $('#inqno').val(response.inqno);
                    $('#propertyname').val(response.propertyName);
                    $('#aptno').val(response.aptno);
                    $('#propertysource').val(response.propertySource);
                    $('#tenantname').val(response.clientName);
                    $('#rentamt').val(response.rent);
                    $('#depositamt').val(response.deposit);
                    $('#clresf').val(response.clientResf);
                    $('#LLRESF').val(response.llresf);
                    var ls = response.leasedate.substring(0, 10);
                    $('#leasestartdate').val(ls);
                    var le = response.leaseenddate.substring(0, 10);
                    $('#leaseenddate').val(le);
                    $('#loivarpath').val(response.loipath);
                    $('#loino').val(response.loiNo);
                    $('#lename').val(response.lename);
                    $('#dept').val(response.dept);

                    $.ajax({
                        type: "POST",
                        url: "@Url.Action("LoadDetFromLcInfoByLoiNo", "Accounts")",
                        data: { loino1: loino1 },
                        success: function (response) {
                            if (response != 0) {
                                $('#lcvarpath').val(response.lcpath);
                                $('#tenantname').val('');
                                $('#tenantname').val(response.tenname);
                            }
                        }
                    })

                    $.ajax({
                        type: "POST",
                        url: "@Url.Action("LoadPayVoucherByLoiNo", "Accounts")",
                        data: { loino1: loino1 },
                        success: function (response) {
                            if (response != 0) {
                                $('#rentpaid').val(response.rentpaid);
                                $('#depositpaid').val(response.deppaid);
                                $('#clresfpaid').val(response.resfpaid);
                                $('#llresfpaid').val(response.llresfpaid);
                            }
                        }
                    })

                    var resfpaid = $('#clresfpaid').val();
                    var clresf = response.clientResf/*$('#clresf').val()*/;
                    if (clresf == resfpaid && clresf != 0) {
                        alert(`${loino1}: RESF is already paid.New Payment will make the mistakes in sum amount`);
                    }

                    var propertysource = $('#propertysource').val();
                    if (propertysource == 'ManagedProperty') {
                        $("#propertyname").prop("disabled", true);
                        $("#aptno").prop("disabled", true);
                    }
                    else {
                        $("#propertyname").prop("disabled", false);
                        $("#aptno").prop("disabled", false);
                    }

                    if (response.dept == 'MARKETING') {
                        chkmarketing.checked = true;
                        chkother.checked = false;
                    }
                    else {
                        chkother.checked = true;
                        chkmarketing.checked = false;
                    }
                }
            })

        });

        $("#vouchertype").change(function () {
            var vouchertype = $('#vouchertype :selected').val();
            if (vouchertype == "-") {
                $('#propertyname').hide();
                $('#propertynameselection').show();
                $('#aptno').hide();
                $('#aptnoselection').show();
                $("#loino1").prop("disabled", true);

                $('#propertyname').val('');
                $('#aptno').val('');
                $('#leasestartdate').val('');
                $('#leaseenddate').val('');
                $('#tenantname').val('');

                loadingpropertyname();
                loadingvoucherno();
            }
            else if (vouchertype == "for original receipts") {
                $('#voucherno').val('-');

                $('#propertyname').show();
                $('#propertynameselection').hide();
                $('#aptno').show();
                $('#aptnoselection').hide();
                $("#loino1").prop("disabled", false);

                $('#propertyname').val('');
                $('#aptno').val('');
                $('#leasestartdate').val('');
                $('#leaseenddate').val('');
                $('#tenantname').val('');
            }
            else {
                $("#loino1").prop("disabled", false);

                $('#propertyname').val('');
                $('#aptno').val('');
                $('#leasestartdate').val('');
                $('#leaseenddate').val('');
                $('#tenantname').val('');

                $('#propertyname').show();
                $('#propertynameselection').hide();
                $('#aptno').show();
                $('#aptnoselection').hide();

                loadingvoucherno();
            }
        })

        $('#propertynameselection').change(function () {
            var propertynameselection = $('#propertynameselection :selected').val();
            $('#propertyname').val(propertynameselection);
            $.ajax({
                type: "POST",
                data: { propertyname: propertynameselection},
                url: "@Url.Action("LoadAptNumber","Common")",
                success: function (response) {
                    $.each(response, function (k, v) {
                        $('#aptnoselection').append(`<option value=${v}>${v}</option>`);
                    })
                }
            })
        })

        $('#aptnoselection').change(function () {
            var propertynameselection = $('#propertynameselection :selected').val();
            var aptnoselection = $('#aptnoselection :selected').val();
            $('#aptno').val(aptnoselection);
            $.ajax({
                type: "POST",
                data: { aptno: aptnoselection, propertyname: propertynameselection},
                url: "@Url.Action("AptChangeEventTempReceipt", "Accounts")",
                success: function (response) {
                    $('#tenantname').val(response.cname);
                    /*var ls = response.leasestart.substring(0, 10);*/
                    $('#leasestartdate').val(response.leasestart);
                    /*var le = response.leaseend.substring(0, 10);*/
                    $('#leaseenddate').val(response.leaseend);
                }
            })
        })

        $('#amounttype').change(function () {
            $('#cash').val('');
            $('#knet').val('');
            $('#cheque').val('');
            $('#bt').val('');
            $('#totalamount').val('');

            var amounttype = $('#amounttype :selected').val();
            if (amounttype == "OTHER") {
                $('#divOther').show();
            }
            else {
                $('#divOther').hide();
            }
        })

        $('#other').change(function () {
            $('#cash').val('');
            $('#knet').val('');
            $('#cheque').val('');
            $('#bt').val('');
            $('#totalamount').val('');
        })

        $('#cash').change(function () {
            var cash = $('#cash').val();
            var knet = $('#knet').val();
            var cheque = $('#cheque').val();
            var bt = $('#bt').val();
            var totalamount = Number(cash) + Number(knet) + Number(cheque) + Number(bt);
            $('#totalamount').val(totalamount);
            //var totalamounttxt = amounttowords(totalamount);
            //$('#totalamounttxt').val(totalamounttxt);
        })
        $('#knet').change(function () {
            var cash = $('#cash').val();
            var knet = $('#knet').val();
            var cheque = $('#cheque').val();
            var bt = $('#bt').val();
            var totalamount = Number(cash) + Number(knet) + Number(cheque) + Number(bt);
            $('#totalamount').val(totalamount);
            //var totalamounttxt = amounttowords(totalamount);
            //$('#totalamounttxt').val(totalamounttxt);
        })
        $('#cheque').change(function () {
            var cash = $('#cash').val();
            var knet = $('#knet').val();
            var cheque = $('#cheque').val();
            var bt = $('#bt').val();
            var totalamount = Number(cash) + Number(knet) + Number(cheque) + Number(bt);
            $('#totalamount').val(totalamount);
            //var totalamounttxt = amounttowords(totalamount);
            //$('#totalamounttxt').val(totalamounttxt);
        })
        $('#bt').change(function () {
            var cash = $('#cash').val();
            var knet = $('#knet').val();
            var cheque = $('#cheque').val();
            var bt = $('#bt').val();
            var totalamount = Number(cash) + Number(knet) + Number(cheque) + Number(bt);
            $('#totalamount').val(totalamount);
            //var totalamounttxt = amounttowords(totalamount);
            //$('#totalamounttxt').val(totalamounttxt);
        })

        $('#btnadd').click(function () {
            var cash = $('#cash').val();
            var knet = $('#knet').val();
            var cheque = $('#cheque').val();
            var bt = $('#bt').val();
            var totalamount = $('#totalamount').val();

            var amounttype = $('#amounttype :selected').val();
            var other = $('#other :selected').val();

            if (amounttype == null || amounttype == '') {
                alert('Select the amount type');
                return false;
            }
            if (totalamount == null || totalamount == '') {
                alert('Total Amount should not be empty');
                return false;
            }


            table = document.getElementById("table2");
            var rows = table.rows;
            for (var i = 1; i < rows.length; i++) {
                var cols = rows[i].cells;
                for (var c = 0; c < cols.length; c++) {
                    if (amounttype == "OTHER") {
                        if (cols[c].innerText == other) {
                            /*return cols[0].innerHTML;*/
                            alert(`${other} is already added`);
                            return false;
                        }
                    }
                    else {
                        if (cols[c].innerText == amounttype) {
                            /*return cols[0].innerHTML;*/
                            alert(`${amounttype} is already added`);
                            return false;
                        }
                    }

                }
            }

            if (amounttype == "OTHER") {

                if (other == null || other == '') {
                    alert('Mention the Other type');
                    return false;
                }
                $('#tbody').append(`<tr>
                                        <td class="vouchertype" style="color: black;">${other}</td>
                                        <td class="totalamount" style="color: black;">${totalamount}</td>
                                        <td class="cash" style="color: black;">${cash}</td>
                                        <td class="knet" style="color: black;">${knet}</td>
                                        <td class="cheque" style="color: black;">${cheque}</td>
                                        <td class="bt" style="color: black;">${bt}</td>
                                    </tr>`);
            }
            else {
                $('#tbody').append(`<tr>
                                        <td class="vouchertype" style="color: black;">${amounttype}</td>
                                        <td class="totalamount" style="color: black;">${totalamount}</td>
                                        <td class="cash" style="color: black;">${cash}</td>
                                        <td class="knet" style="color: black;">${knet}</td>
                                        <td class="cheque" style="color: black;">${cheque}</td>
                                        <td class="bt" style="color: black;">${bt}</td>
                                    </tr>`);
            }

            $('#cash').val('');
            $('#knet').val('');
            $('#cheque').val('');
            $('#bt').val('');
            $('#totalamount').val('');
        })

        $('#multiplepayment').click(function () {
            var multiplepayment = document.getElementById('multiplepayment').checked;
            if (multiplepayment == true) {
                $('#btnadd').show();
                $('#table2').show();
            }
            else {
                $('#btnadd').hide();
                $('#table2').hide();
            }
        })

        $('#chkcash').click(function () {
            var chkcash = document.getElementById('chkcash').checked;
            if (chkcash == true) {
                $("#cash").prop("disabled", false);
            }
            else {
                $("#cash").prop("disabled", true);
            }
        })
        $('#chkknet').click(function () {
            var chkknet = document.getElementById('chkknet').checked;
            if (chkknet == true) {
                $("#knet").prop("disabled", false);
            }
            else {
                $("#knet").prop("disabled", true);
            }
        })
        $('#chkcheque').click(function () {
            var chkcheque = document.getElementById('chkcheque').checked;
            if (chkcheque == true) {
                $("#cheque").prop("disabled", false);
            }
            else {
                $("#cheque").prop("disabled", true);
            }
        })
        $('#chkbt').click(function () {
            var chkbt = document.getElementById('chkbt').checked;
            if (chkbt == true) {
                $("#bt").prop("disabled", false);
            }
            else {
                $("#bt").prop("disabled", true);
            }
        })




        $('#btncreate').click(function () {
            var arrayVouchertype = [];
            var arrayTotalamt = [];
            var arrayCash = [];
            var arrayKnet = [];
            var arrayCheque = [];
            var arrayBt = [];

            var tomail = $('#tomail').val();
            var ccmail = $('#ccmail').val();
            var propertyname = $('#propertyname').val();
            var chkcash = document.getElementById('chkcash').checked;
            var chkknet = document.getElementById('chkknet').checked;
            var chkcheque = document.getElementById('chkcheque').checked;
            var chkbt = document.getElementById('chkbt').checked;
            var cash = $('#cash').val();
            var knet = $('#knet').val();
            var cheque = $('#cheque').val();
            var bt = $('#bt').val();
            var totalamount = $('#totalamount').val();
            var remarks = $('#remarks').val();
            var vouchertype = $('#vouchertype :selected').val();
            var loino1 = $('#loino1 :selected').val();
            var amounttype = $('#amounttype :selected').val();
            var other = $('#other :selected').val();
            var voucherno = $('#voucherno').val();
            var fromdate = $('#fromdate').val();
            var todate = $('#todate').val();
            var multiplepayment = document.getElementById('multiplepayment').checked;
            var loino = $('#loino').val();
            var inqno = $('#inqno').val();
            var vouchermadeon = $('#vouchermadeon').val();
            var tenantname = $('#tenantname').val();
            var aptno = $('#aptno').val();

            var tablebody = document.getElementById("tbody");
            var rowslength = tablebody.rows.length;

            $('#tbody tr').each(function () {
                var tableVouchertype = $(this).find('.vouchertype').html();
                arrayVouchertype.push(tableVouchertype);

                var tableTotalamt = $(this).find('.totalamount').html();
                arrayTotalamt.push(tableTotalamt);

                var tableCash = $(this).find('.cash').html();
                arrayCash.push(tableCash);

                var tableKnet = $(this).find('.knet').html();
                arrayKnet.push(tableKnet);

                var tableCheque = $(this).find('.cheque').html();
                arrayCheque.push(tableCheque);

                var tableBt = $(this).find('.bt').html();
                arrayBt.push(tableBt);
            });

            if (propertyname == null || propertyname == "") {
                alert('Enter the Propertyname');
                return false;
            }
            var cashtype = "-";
            if (chkcash == true) {
                if (cash == null) {
                    alert('Enter the Cash amt');
                    return false;
                }
            }
            if (chkknet == true) {
                if (knet == null) {
                    alert('Enter the KNET amt');
                    return false;
                }
            }
            if (chkcheque == true) {
                if (cheque == null) {
                    alert('Enter the Cheque amt');
                    return false;
                }
            }
            if (chkbt == true) {
                if (bt == null) {
                    alert('Enter the BT amt');
                    return false;
                }
            }

            if (vouchertype != '-') {
                if (loino1 == null) {
                    alert('Select the LOI NO');
                    return false;
                }
            }

            if (amounttype == null) {
                alert('Select Voucher Type');
                return false;
            }

            if (totalamount == null) {
                alert('Total Amount should not be empty');
                return false;
            }

            if (voucherno == null) {
                alert('Enter the Voucher No');
                return false;
            }

            if (fromdate == null) {
                alert('Voucher From date is required');
                return false;
            }

            if (todate == null) {
                alert('Voucher To date is required');
                return false;
            }

            $.ajax({
                type: "POST",
                data: { voucherno: voucherno, multiplepayment: multiplepayment, rowslength: rowslength, loino: loino, inqno: inqno, loino1: loino1, arrayVouchertype: arrayVouchertype, arrayTotalamt: arrayTotalamt, arrayCash: arrayCash, arrayKnet: arrayKnet, arrayCheque: arrayCheque, arrayBt: arrayBt, other: other, amounttype: amounttype, totalamount: totalamount, vouchermadeon: vouchermadeon, remarks: remarks, fromdate: fromdate, todate: todate, cashtype: cashtype, tenantname: tenantname, propertyname: propertyname, aptno: aptno, cash: cash, knet: knet, cheque: cheque, bt: bt, tomail: tomail, ccmail: ccmail},
                url: "@Url.Action("CreateTemporaryReceipt", "Accounts")",
                success: function (response) {
                    alert(response);
                    loadpayvoucherbyloino(loino1);
                }
            })
        })

    });
</script>



