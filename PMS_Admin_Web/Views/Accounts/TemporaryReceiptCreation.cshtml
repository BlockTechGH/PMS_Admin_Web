<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script>
    function loadnotcancelledloi() {
        $.ajax({
            type: "GET",
            url: "@Url.Action("LoiNotCancelled","Common")",
            success: function (response) {
                $.each(response, function (k, v) {
                    $('#loino1').append(`<option value="${v}">${v}</option>`);
                });
            }
        })
    }

    function loadingvoucherno() {
        $.ajax({
            type: "POST",
            url: "@Url.Action("LoadVoucherNo", "Accounts")",
            success: function (response) {
                $('#voucherno').val(response);
            }
        })
    }

    function loadingpropertyname() {
        $.ajax({
            type: "GET",
            url: "@Url.Action("LoadPropertyName","Common")",
            success: function (response) {
                $.each(response, function (k, v) {
                    $('#propertynameselection').append(`<option value="${v}">${v}</option>`);
                })
            }
        })
    }

    function loadpayvoucherbyloino(loino1) {
        $.ajax({
            type: "POST",
            url: "@Url.Action("LoadPayVoucherByLoiNo", "Accounts")",
            data: { loino1: loino1 },
            success: function (response) {
                if (response != 0) {
                    $('#rentpaid').val(response.rentpaid);
                    $('#depositpaid').val(response.deppaid);
                    $('#clresfpaid').val(response.resfpaid);
                    $('#llresfpaid').val(response.llresfpaid);
                }
            }
        })
    }

    //function amounttowords(num) {
    //    var a = ['', 'one ', 'two ', 'three ', 'four ', 'five ', 'six ', 'seven ', 'eight ', 'nine ', 'ten ', 'eleven ', 'twelve ', 'thirteen ', 'fourteen ', 'fifteen ', 'sixteen ', 'seventeen ', 'eighteen ', 'nineteen '];
    //    var b = ['', '', 'twenty', 'thirty', 'forty', 'fifty', 'sixty', 'seventy', 'eighty', 'ninety'];

    //    if ((num = num.toString()).length > 9) return 'overflow';
    //    n = ('000000000' + num).substr(-9).match(/^(\d{2})(\d{2})(\d{2})(\d{1})(\d{2})$/);
    //    if (!n) return; var str = '';
    //    str += (n[1] != 0) ? (a[Number(n[1])] || b[n[1][0]] + ' ' + a[n[1][1]]) + 'crore ' : '';
    //    str += (n[2] != 0) ? (a[Number(n[2])] || b[n[2][0]] + ' ' + a[n[2][1]]) + 'lakh ' : '';
    //    str += (n[3] != 0) ? (a[Number(n[3])] || b[n[3][0]] + ' ' + a[n[3][1]]) + 'thousand ' : '';
    //    str += (n[4] != 0) ? (a[Number(n[4])] || b[n[4][0]] + ' ' + a[n[4][1]]) + 'hundred ' : '';
    //    str += (n[5] != 0) ? ((str != '') ? 'and ' : '') + (a[Number(n[5])] || b[n[5][0]] + ' ' + a[n[5][1]]) + 'only ' : '';
    //    return str;
