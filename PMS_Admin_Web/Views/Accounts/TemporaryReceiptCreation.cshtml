<div class="col-lg-12">
    <form asp-action="TemporaryReceiptCreation" asp-controller="Accounts">
        <input id="tomail" type="hidden" asp-for="tomailid" />
        <input id="ccmail" type="hidden" asp-for="ccmailid" />
        <input type="hidden" asp-for="tomailidlist" />
        <input type="hidden" asp-for="ccmailidlist" />

        <input id="propertysource" type="hidden" asp-for="propertysource" />
        <input id="rentamt" type="hidden" />
        <input id="depositamt" type="hidden" />
        <input id="clresf" type="hidden" />
        <input id="LLRESF" type="hidden" />
        <input id="loino" type="hidden" asp-for="paymentvoucher.Loino" />
        <input id="lename" type="hidden" />
        <input id="dept" type="hidden" asp-for="paymentvoucher.Dept" />
        <input id="loivarpath" type="hidden" />
        <input id="lcvarpath" type="hidden" />
        <div class="card" style="color:burlywood">
            <div class="card-header" style="background-color: darksalmon;">
                <h5 class="text-center" style="color:black"><strong>Temporary Receipt Creation</strong></h5>
            </div>
            <div class="card-body">
                <br />
                <div class="col-lg-12">
                    <div class="row">
                        <div class="col-lg-3">
                            <div class="form-group">
                                <label class="control-label text-black-50">Voucher Type</label>
                                <select id="vouchertype" asp-for="paymentvoucher.Ptype" class="form-control" style="border-radius: 10px;">
                                    <option hidden>--Select--</option>
                                    <option value="Initial Payment">Initial Payment</option>
                                    <option value="-">-</option>
                                    <option value="for original receipts">for original receipts</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-lg-3">
                            <div class="form-group">
                                <label class="control-label text-black-50">Voucher No</label>
                                <input id="voucherno" asp-for="paymentvoucher.Voucherno" class="form-control" type="text" style="border-radius: 10px;" />
                            </div>
                        </div>
                        <div class="col-lg-3">
                            <div class="form-group">
                                <label class="control-label text-black-50">Voucher Made On</label>
                                <input id="vouchermadeon" asp-for="paymentvoucher.PaymentDate" class="form-control" type="date" style="border-radius: 10px;" />
                            </div>
                        </div>
                    </div>
                </div>

                <ul class="nav nav-tabs" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" data-toggle="tab" href="#BasicDetails">Basic Details</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="tab" href="#PaymentDetails">Payment Details</a>
                    </li>
                </ul>
                <div class="tab-content">
                    <div class="tab-pane active" id="BasicDetails">
                        <div class="col-lg-12">
                            <br />
                            <div class="row">
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Select LOI No</label>
                                        <select id="loino1" asp-for="paymentvoucher.LoiNo1" class="form-control" style="border-radius: 10px;">
                                            <option hidden>--Select--</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Inq No</label>
                                        <input id="inqno" asp-for="paymentvoucher.Inqno" class="form-control" type="text" style="border-radius: 10px;" />
                                    </div>
                                </div>

                            </div>
                            <div class="row">

                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Property Name</label>
                                        <input id="propertyname" asp-for="paymentvoucher.Pname" type="text" class="form-control" style="border-radius: 10px;" />

                                        <select id="propertynameselection" asp-for="paymentvoucher.Pname" class="form-control" style="border-radius: 10px;">
                                            <option value="" hidden>--Select--</option>
                                        </select>
                                        @*<input type="hidden" id="propertyname_hidden" asp-for="paymentvoucher.Pname" />*@
                                    </div>
                                </div>
                                @*<div class="col-lg-3" style="margin-top: 38px;">
                                    <div class="form-group">
