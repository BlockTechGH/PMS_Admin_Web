@model PMS_Admin_Web.Models.Marketing.QuestionsModel

@{
    ViewData["Title"] = "Questionnaire";
}

@if (ViewBag.Message != null)
{
    <script type="text/javascript">
        window.onload = function () {
            alert("@ViewBag.Message");
        };
    </script>
}

<script>
    function loadquestionform() {
        var inqno = $('#inqno').val();
        /*alert(inqno);*/
        $.ajax({
            type: "POST",
            data: { inqno: inqno},
            url: "@Url.Action("LoadQuestions", "Marketing")",
            success: function (response) {
                if (response != 0) {
                    $('#btnupdate').show();
                    $('#btncreate').hide();

                    $('#fname').val(response.fname);
                    $('#lname').val(response.lname);
                    $('#number').val(response.number);
                    $('#currentloc').val(response.currentloc);
                    $('#whenapt').val(response.whenapt);
                    $('#company').val(response.company);
                    $('#findwhere').val(response.findwhere);
                    $('#inkwt').val(response.inkwt);
                    $('#ploc').val(response.ploc);
                    $('#TIMETXT').val(response.kvisit);
                    $('#KIDSTXT').val(response.school);
                    $('#SCHOOLTXT').val(response.closeschool);
                    $('#facilities').val(response.facilities);
                    $('#distance').val(response.distance);
                    $('#dimen').val(response.dimen);
                    $('#notice').val(response.notice);

                    if ($.trim(response.bed) == "1 Bedroom") {
                        $("select#bed").prop('selectedIndex', 1);
                    }
                    else if ($.trim(response.bed) == "2 Bedroom") {
                        $("select#bed").prop('selectedIndex', 2);
                    }
                    else if ($.trim(response.bed) == "3 Bedroom") {
                        $("select#bed").prop('selectedIndex', 3);
                    }
                    else if ($.trim(response.bed) == "4 Bedroom") {
                        $("select#bed").prop('selectedIndex', 4);
                    }
                    else if ($.trim(response.bed) == "5 Bedroom") {
                        $("select#bed").prop('selectedIndex', 5);
                    }
                    else if ($.trim(response.bed) == "6 Bedroom") {
                        $("select#bed").prop('selectedIndex', 6);
                    }
                    else if ($.trim(response.bed) == "7 Bedroom") {
                        $("select#bed").prop('selectedIndex', 7);
                    }
                    else if ($.trim(response.bed) == "8 Bedroom") {
                        $("select#bed").prop('selectedIndex', 8);
                    }
                    else {
                        $("select#bed").prop('selectedIndex', 9);//Studio
                    }

                    if ($.trim(response.type) == "SF") {
                        $("select#type").prop('selectedIndex', 1);
                    }
                    else if ($.trim(response.type) == "FF") {
                        $("select#type").prop('selectedIndex', 2);
                    }
                    else if ($.trim(response.type) == "UF") {
                        $("select#type").prop('selectedIndex', 3);
                    }
                    else if ($.trim(response.type) == "SF/FF/UF") {
                        $("select#type").prop('selectedIndex', 4);
                    }

                    if ($.trim(response.contract) == "YES") {
                        $('#undercontractyes').attr('checked', true);
                        $('#LONGTXT').val(response.howlong);
                        $('#LONGTXT').attr("disabled", false);
                    }
                    else {
                        $('#undercontractno').attr('checked', true);
                    }

                    if ($.trim(response.maid) == "YES") {
                        $('#maidyes').attr('checked', true);
                    }
                    else {
                        $('#maidno').attr('checked', true);
                    }

                    if ($.trim(response.newbuild) == "YES") {
                        $('#newbuildyes').attr('checked', true);
                    }
                    else {
                        $('#newbuildno').attr('checked', true);
                    }

                    if ($.trim(response.pet) == "YES") {
                        $('#petyes').attr('checked', true);
                        $('#KINDTXT').val(response.kind);
                        $('#KINDTXT').attr("disabled", false);
                    }
                    else {
                        $('#petno').attr('checked', true);
                    }

                    if ($.trim(response.otheragent) == "YES") {
                        $("select#otheragent").prop('selectedIndex', 1);
                    }
                    else {
                        $("select#otheragent").prop('selectedIndex', 2);
                    }

                    if ($.trim(response.seen) == "YES") {
                        $('#aptseenyes').attr('checked', true);
                        $('#FEEDBACKTXT').val(response.feedbacks);
                        $('#FEEDBACKTXT').attr("disabled", false);
                    }
                    else {
                        $('#aptseenno').attr('checked', true);
                    }

                    if ($.trim(response.maritial) == "YES") {
                        $('#marriedyes').attr('checked', true);
                        $('#REQTXT').attr("disabled", false);
                        $('#wife').attr("disabled", false);
                    }
                    else {
                        $('#marriedno').attr('checked', true);
                    }

                    if ($.trim(response.wife) == "YES") {
                        $("select#wife").prop('selectedIndex', 1);
                        $('#REQTXT').val(response.wifereq);
                    }
                    else {
                        $("select#wife").prop('selectedIndex', 2);
                    }

                    if (response.find == "Advertising Portal") {
                        $("select#find").prop('selectedIndex', 1);
                    }
                    else if (response.find == "Referral") {
                        $("select#find").prop('selectedIndex', 2);
                    }
                    else if (response.find == "Social Media") {
                        $("select#find").prop('selectedIndex', 3);
                    }
                    else if (response.find == "Other") {
                        $("select#find").prop('selectedIndex', 4);
                    }

                    if ($.trim(response.idea) == "YES") {
                        $("select#idea").prop('selectedIndex', 1);
                    }
                    else {
                        $("select#idea").prop('selectedIndex', 2);
                    }

                    if ($.trim(response.kids) == "YES") {
                        $("select#kids").prop('selectedIndex', 1);
                    }
                    else {
                        $("select#kids").prop('selectedIndex', 2);
                    }

                    if ($.trim(response.final) == "Individual") {
                        $("select#final").prop('selectedIndex', 1);
                    }
                    else {
                        $("select#final").prop('selectedIndex', 2);
                    }
                }
                else {
                    /*return false;*/
                    $('#btnupdate').hide();
                    $('#btncreate').show();
                }
            }
        })
    }

