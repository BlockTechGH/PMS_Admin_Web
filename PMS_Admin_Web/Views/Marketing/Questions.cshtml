                    return false;
                }
            }
            if (facilities == "" || facilities == undefined) {
                alert("Specify the facilities requiring");
                return false;
            }
            if (distance == undefined || distance == "") {
                alert('Specify where the client would like to stay?');
                return false;
            }
            if (dimen == undefined || dimen == "") {
                alert("Specify is there any idea of the dimension of the apartment");
                return false;
            }
            if (notice == "" || notice == undefined) {
                alert('How long the notice period is given for the client in the current apartment');
                return false;
            }
            if (final == undefined || final == "") {
                alert('Specify the contract will be client name or company name?');
                return false;
            }
            if (find == undefined || find == "") {
                alert('Specify where did they find us');
                return false;
            }
            if (findwhere == undefined || findwhere == "") {
                alert('Specify where did they find us');
                return false;
            }

            $.ajax({
                type: "POST",
                data: { inqno: inqno, fname: fname, lname: lname, number: number, currentloc: currentloc, whenapt: whenapt, company: company, undercontract: undercontract, LONGTXT: LONGTXT, bed: bed, type: type, married: married, wife: wife, REQTXT: REQTXT, inkwt: inkwt, idea: idea, ploc: ploc, kids: kids, TIMETXT: TIMETXT, KIDSTXT: KIDSTXT, SCHOOLTXT: SCHOOLTXT, maid: maid, newb: newb, pet: pet, KINDTXT: KINDTXT, otheragent: otheragent, chkseenapts: chkseenapts, FEEDBACKTXT: FEEDBACKTXT, facilities: facilities, distance: distance, dimen: dimen, notice: notice, final: final, find: find, findwhere: findwhere},
