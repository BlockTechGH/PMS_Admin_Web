                                        <input class="form-control" asp-for="moveout.Moidate" type="date" style="border-radius: 10px;" />
                                    </div>
                                </div>
                            </div>
                            <br />

                            <label><strong>Moving Out Inventory</strong></label>
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Moving Out Inventory Report</label>
                                        @if (Model.moveout.Moi != null)
                                        {
                                            <input type="checkbox" checked />
                                        }
                                        else
                                        {
                                            <input type="checkbox" />
                                        }
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <input asp-for="moveout.Moi" type="file" />
                                </div>
                                <div class="col-lg-3">
                                    <button class="btn btn-primary">Upload</button>
                                </div>

                            </div>
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Moving Out Inventory List/Breakdown</label>
                                        @if (Model.moveout.Moilist != null)
                                        {
                                            <input type="checkbox" checked />
                                        }
                                        else
                                        {
                                            <input type="checkbox" />
                                        }
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <input asp-for="moveout.Moilist" type="file" />
                                </div>
                                <div class="col-lg-3">
                                    <button class="btn btn-primary">Upload</button>
                                </div>

                            </div>

                            <label><strong>Does Tenant agree for the Refurbishing cost?</strong></label>
                            <div class="row">
                                <div class="col-lg-3">
                                    <div class="row">
                                        @if (Model.moveout.Tenantrefurb != null)
                                        {
                                            @if (Model.moveout.Tenantrefurb.Trim() == "YES")
                                            {
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">Yes</label>
                                                        <input asp-for="moveout.Tenantrefurb" type="radio" value="Yes" checked>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">No</label>
                                                        <input asp-for="moveout.Tenantrefurb" type="radio" value="No">
                                                    </div>
                                                </div>
                                            }
                                            else if (Model.moveout.Tenantrefurb.Trim() == "NO")
                                            {
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">Yes</label>
                                                        <input asp-for="moveout.Tenantrefurb" type="radio" value="Yes">
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">No</label>
                                                        <input asp-for="moveout.Tenantrefurb" type="radio" value="No" checked>
                                                    </div>
                                                </div>
                                            }
                                        }
                                        else
                                        {
                                            <div class="col-lg-6">
                                                <div class="form-group">
                                                    <label class="control-label text-black-50">Yes</label>
                                                    <input asp-for="moveout.Tenantrefurb" type="radio" value="Yes">
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="form-group">
                                                    <label class="control-label text-black-50">No</label>
                                                    <input asp-for="moveout.Tenantrefurb" type="radio" value="No">
                                                </div>
                                            </div>
                                        }

                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Refurbishing Breakdown</label>
                                        @if (Model.moveout.Refurbbrkdown != null)
                                        {
                                            <input type="checkbox" checked />
                                        }
                                        else
                                        {
                                            <input type="checkbox" />
                                        }
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <input asp-for="moveout.Refurbbrkdown" type="file" />
                                </div>
                                <div class="col-lg-3">
                                    <button class="btn btn-primary">Upload</button>
                                </div>

                            </div>
                        </div>



                    </div>



