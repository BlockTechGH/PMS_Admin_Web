                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Vacating Notice</label>
                                        <input asp-for="moveout.Vn" class="form-control" type="text" style="border-radius: 10px;" />
                                        @*<span asp-validation-for="LcInfo.Tenname" class="text-danger"></span>*@
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Move Date</label>
                                        <input class="form-control" type="date" asp-for="MoveOutDate" style="border-radius: 10px;" />
                                        @*<span asp-validation-for="LcInfo.Tenname" class="text-danger"></span>*@
                                    </div>
                                </div>
                            </div>
                            <label><strong>Management Approval/Approved Vacating Notice</strong></label>
                            <div class="row">
                                <div class="col-lg-3">
                                    <div class="row">
                                        @if (Model.moveout.Bmapproved != null)
                                        {
                                            @if (Model.moveout.Bmapproved.Trim() == "YES")
                                            {
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">Yes</label>
                                                        <input type="radio" asp-for="moveout.Bmapproved" value="YES" checked>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">No</label>
                                                        <input type="radio" asp-for="moveout.Bmapproved" value="NO">
                                                    </div>
                                                </div>
                                            }
                                            else if (Model.moveout.Bmapproved.Trim() == "NO")
                                            {
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">Yes</label>
                                                        <input type="radio" asp-for="moveout.Bmapproved" value="YES">
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">No</label>
                                                        <input type="radio" asp-for="moveout.Bmapproved" value="NO" checked>
                                                    </div>
                                                </div>
                                            }
                                        }
                                        else
                                        {
                                            <div class="col-lg-6">
                                                <div class="form-group">
                                                    <label class="control-label text-black-50">Yes</label>
                                                    <input type="radio" asp-for="moveout.Bmapproved" value="YES">
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="form-group">
                                                    <label class="control-label text-black-50">No</label>
                                                    <input type="radio" asp-for="moveout.Bmapproved" value="NO">
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Management Approval</label>
                                        @if (Model.moveout.Bmapproval != null)
                                        {
                                            <input type="checkbox" checked />
                                        }
                                        else
                                        {
                                            <input type="checkbox" />
                                        }

                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <input asp-for="moveout.Bmapproval" type="file" />
                                </div>
                                <div class="col-lg-3">
                                    <button class="btn btn-primary">Upload</button>
                                </div>
                            </div>

                            <label><strong>MII Status</strong></label>
                            <div class="row">
                                <div class="col-lg-3">
                                    <div class="row" style="width: 500px;">
                                        @if (Model.moveout.Moistatus != null)
                                        {
                                            @if (Model.moveout.Moistatus.Trim() == "YES")
                                            {
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">Moving Out Inventory Done</label>
                                                        <input asp-for="moveout.Moistatus" type="radio" value="Yes" checked>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">Not Done</label>
                                                        <input asp-for="moveout.Moistatus" type="radio" value="No">
                                                    </div>
                                                </div>
                                            }
                                            else if (Model.moveout.Moistatus.Trim() == "NO")
                                            {
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">Moving Out Inventory Done</label>
                                                        <input asp-for="moveout.Moistatus" type="radio" value="Yes">
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="control-label text-black-50">Not Done</label>
                                                        <input asp-for="moveout.Moistatus" type="radio" value="No" checked>
                                                    </div>
                                                </div>
                                            }
                                        }
                                        else
                                        {
                                            <div class="col-lg-6">
                                                <div class="form-group">
                                                    <label class="control-label text-black-50">Moving Out Inventory Done</label>
                                                    <input asp-for="moveout.Moistatus" type="radio" value="Yes">
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="form-group">
                                                    <label class="control-label text-black-50">Not Done</label>
                                                    <input asp-for="moveout.Moistatus" type="radio" value="No">
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                                <div class="col-lg-3">
