                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Reference No</label>
                                        <input class="form-control" type="text" asp-for="PropertyRef" style="border-radius: 10px;" />
                                        @*<span asp-validation-for="LcInfo.Tenname" class="text-danger"></span>*@
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label class="control-label text-black-50">Tenant Name</label>
                                        <input asp-for="TenantName" class="form-control" type="text" style="border-radius: 10px;" />
