@{
    ViewData["Title"] = "Moving Out";
}

<style>
    .button2:hover {
        box-shadow: 0 12px 16px 0 rgba(0,0,0,0.24),0 17px 50px 0 rgba(0,0,0,0.19);
    }
</style>

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="col-lg-12">
                <form>
                    <br />
                    <br />
                    <div class="row">
                        <div class="col-lg-3">
                            <button class="btn btn-primary button2" style="width: 250px; height: 105px; border-radius: 12px; background-color: #e27d60" asp-action="PaNewVacating" asp-controller="PA">
                                <div>
                                    <div>
                                        <i class="fa fa-align-justify" style="font-size:24px"></i> <i class="fa fa-plus" aria-hidden="true" style="font-size:15px"></i>
                                    </div>
                                    <div>
                                        <strong>New Vacating</strong>
                                    </div>
                                </div>
                            </button>
                        </div>
                        <div class="col-lg-3">
                            <button class="btn btn-primary button2" style="width: 250px; height: 105px; border-radius: 12px; background-color: #e27d60" asp-action="PaVacatingList" asp-controller="PA">
                                <div>
                                    <div>
                                        <i class="fa fa-list" style="font-size:24px"></i>
                                    </div>
                                    <div>
                                        <strong>Vacating List</strong>
                                    </div>
                                </div>
                            </button>
                        </div>
                    </div>
                    <br />
                </form>
            </div>
        </div>
    </div>
</div>